<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javatermproject.TransferMovieController">
   <children>
      <Label alignment="CENTER" layoutX="45.0" prefHeight="44.0" prefWidth="578.0" text="You want to transfer the movie">
         <font>
            <Font name="System Bold" size="30.0" />
         </font>
      </Label>
      <Label fx:id="movieName" alignment="CENTER" layoutX="151.0" layoutY="52.0" prefHeight="45.0" prefWidth="346.0">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <Label alignment="CENTER" layoutX="99.0" layoutY="150.0" prefHeight="30.0" prefWidth="469.0" text="To which production company you want to transfer?">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <ChoiceBox fx:id="choiceBox" layoutX="93.0" layoutY="196.0" prefHeight="35.0" prefWidth="369.0" />
      <Button layoutX="479.0" layoutY="196.0" mnemonicParsing="false" onAction="#transfer" prefHeight="35.0" prefWidth="96.0" text="Transfer">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Button>
      <Button layoutX="13.0" layoutY="261.0" mnemonicParsing="false" onAction="#click" prefHeight="25.0" prefWidth="86.0" text="Back" />
   </children>
</AnchorPane>
